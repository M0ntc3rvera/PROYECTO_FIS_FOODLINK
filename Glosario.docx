#upsellBannerForPad button p, #upsellBannerForPhone button p { margin: 0px; font-size: 16px; }

#upsellBannerForPad { width: 310px; height: 254px; top: calc(-127px + 50vh); }

[dir="ltr"] #upsellBannerForPad { left: calc(-154px + 50vw); }

